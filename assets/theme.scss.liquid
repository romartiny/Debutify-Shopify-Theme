      @include transform(translatey(-($heightHeaderScrolling - $heightHeader)));
    }
    
    .site-header{
      height: $heightHeader;
      position:absolute;
      width: 100%;
      top: ($heightHeaderScrolling - $heightHeader);
      @include shadow($shadowBottom);
    }

    //show default logo - hide inverted logo
