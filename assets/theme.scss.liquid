    .default-logo{
      display:none;
    }
    .inverted-logo{
      display:none;
    }
    .mobile-logo{
      display: block;
    }
  }

  //reaply style to overwrite hero header
  .site-header{
    background-color: $colorNav!important;
  }
  .site-nav__link, .site-header__logo-link{
